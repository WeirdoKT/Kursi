i = 0
my_name = "Kalvis"
while i != 6:
    if i != 0:
        print(f"{my_name} #{i}")
        i += 1
    else: i +=1

my_name_x_times = my_name * 6
print(my_name_x_times) # dubplicates and concatenates string the amount of times it's multiplied

seconds = 365 * 24 * 60 * 60
print(seconds)

googol = 10**100
print(googol)

Ba = "Ba"
na = "na"
print(f"{Ba}"+na*4)
